<h1> Coupon Form </h1>

<%= form_tag coupons_path do %>
    
    <%= text_field_tag 'coupon[coupon_code]', placeholder="Coupon Code" %>
    <%= text_field_tag 'coupon[store]', placeholder="Store Name" %>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>

    <%= submit_tag "Submit Coupon" %>

<% end %>